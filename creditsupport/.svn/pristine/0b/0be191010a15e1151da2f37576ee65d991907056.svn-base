#indexReportAttrTable1 th.tableColor,
#indexReportAttrTable2 th.tableColor{
  border-right: none;
  font-size:14px;
  font-weight: none;
}
#indexReportTable tbody tr:first-child td,
#indexReportAttrTable1 tbody tr:first-child td,
#indexReportAttrTable2 tbody tr:first-child td,
