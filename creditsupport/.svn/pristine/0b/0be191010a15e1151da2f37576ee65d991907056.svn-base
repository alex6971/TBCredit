.likeTables {
  width:290px;
  margin:0 auto;
}
.likeTables > div:first-child{
  height: 105px;
  padding: 25px 10px;
  font-size: 14px;
  color:#333;
  font-weight: bold;
  text-align: center;
  /*border-right: 5px solid #80deea;*/
  background-color: #eee;
  cursor: pointer;
  border-top: 1px solid #eff3f8;
  border-bottom: 1px solid #eff3f8;
}
.likeTables > div.active {
  border: none;
  /*  background-color: #4dd0e1; */
  color: #fff;
}
.likeTables > div:last-child > div{
  height: 35px;
  width: 200px;
  overflow: hidden;
  font-size: 12px;
}
.likeTables > div:last-child > div > span {
  /*width: 50%;*/
  float: left;
  padding-left: 10px;
  border: 1px solid #eff3f8;
  border-left: none;
  height: 35px;
  line-height: 35px;
}
.likeTables > div:last-child > div:first-child > span{
  border-bottom: none;
}
.likeTables > div:last-child > div:last-child > span {
  border-top: none;
}
.likeTables > div:last-child > div > span:first-child {
  background-color: #fcfcfc;
  width: 60%;
}
.likeTables > div:last-child > div > span:last-child {
  width: 40%;
}
