package com.yodata.creditsupport.orm.vo;

import java.io.Serializable;
import java.math.BigDecimal;

public class IndicatorAttr implements Serializable {

	/**
	 * 序列化
	 */
	private static final long serialVersionUID = -2105473271409226914L;

	//基本信息
	private String regId;
	private Integer dbsiOperatingYear;     //经营时长（年）
	private Integer dbsiShareholderNum;    //股东数量（人）
	private BigDecimal dbsiLegalStock;     //法人股份占比（%）
	
	//交易行为
	private BigDecimal dtdbOrderAvgNum;       //月出票量（张）
	private BigDecimal dtdbOrderAmount;    //月销售额（万元）
	private BigDecimal dtdbOrderAvgRate;   //月出票增长率（%）
	
    //（3）履约能力
	private BigDecimal dfncCurrentAssets;  //流动资产总额（万元）
	private BigDecimal dfncCurrentLiability; //流动负债总额（万元）
	private BigDecimal dfncBankCash;       //货币资金（万元）
	
    //（4）特殊事项
	private Integer dsmiBreachNum;         //法院裁决违约（次）
	private Integer dsmiCreditbreakNum;    //法院裁决失信（次）
	
	//信用历史
//	private Integer lperCrecardRec;
//	private Integer entCreRec;
//	private Integer ryxBusOverdueRec;
	
    
    public String getRegId() {
        return regId;
    }
    public void setRegId(String regId) {
        this.regId = regId;
    }
    public Integer getDbsiOperatingYear() {
        return dbsiOperatingYear;
    }
    public void setDbsiOperatingYear(Integer dbsiOperatingYear) {
        this.dbsiOperatingYear = dbsiOperatingYear;
    }
    public Integer getDbsiShareholderNum() {
        return dbsiShareholderNum;
    }
    public void setDbsiShareholderNum(Integer dbsiShareholderNum) {
        this.dbsiShareholderNum = dbsiShareholderNum;
    }
    public BigDecimal getDbsiLegalStock() {
        return dbsiLegalStock;
    }
    public void setDbsiLegalStock(BigDecimal dbsiLegalStock) {
        this.dbsiLegalStock = dbsiLegalStock;
    }
    public BigDecimal getDtdbOrderAvgNum() {
        return dtdbOrderAvgNum;
    }
    public void setDtdbOrderAvgNum(BigDecimal dtdbOrderAvgNum) {
        this.dtdbOrderAvgNum = dtdbOrderAvgNum;
    }
    public BigDecimal getDtdbOrderAmount() {
        return dtdbOrderAmount;
    }
    public void setDtdbOrderAmount(BigDecimal dtdbOrderAmount) {
        this.dtdbOrderAmount = dtdbOrderAmount;
    }
    public BigDecimal getDtdbOrderAvgRate() {
        return dtdbOrderAvgRate;
    }
    public void setDtdbOrderAvgRate(BigDecimal dtdbOrderAvgRate) {
        this.dtdbOrderAvgRate = dtdbOrderAvgRate;
    }
    public BigDecimal getDfncCurrentAssets() {
        return dfncCurrentAssets;
    }
    public void setDfncCurrentAssets(BigDecimal dfncCurrentAssets) {
        this.dfncCurrentAssets = dfncCurrentAssets;
    }
    public BigDecimal getDfncCurrentLiability() {
        return dfncCurrentLiability;
    }
    public void setDfncCurrentLiability(BigDecimal dfncCurrentLiability) {
        this.dfncCurrentLiability = dfncCurrentLiability;
    }
    public BigDecimal getDfncBankCash() {
        return dfncBankCash;
    }
    public void setDfncBankCash(BigDecimal dfncBankCash) {
        this.dfncBankCash = dfncBankCash;
    }
    public Integer getDsmiBreachNum() {
        return dsmiBreachNum;
    }
    public void setDsmiBreachNum(Integer dsmiBreachNum) {
        this.dsmiBreachNum = dsmiBreachNum;
    }
    public Integer getDsmiCreditbreakNum() {
        return dsmiCreditbreakNum;
    }
    public void setDsmiCreditbreakNum(Integer dsmiCreditbreakNum) {
        this.dsmiCreditbreakNum = dsmiCreditbreakNum;
    }
   
}